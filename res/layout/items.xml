<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent">
	<ImageView android:id="@+id/imageView1"
		android:layout_height="wrap_content" android:src="@drawable/icon"
		android:layout_width="wrap_content" android:layout_alignParentTop="true"
		android:layout_alignParentLeft="true"></ImageView>
	<TextView android:layout_width="wrap_content" android:id="@+id/textView5"
		android:layout_height="wrap_content" android:text="TextView"
		android:textSize="18sp" android:layout_below="@+id/textView4"
		android:layout_alignLeft="@+id/textView4"></TextView>

	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/textView2"
		android:text="TextView" android:textSize="18sp" android:layout_below="@+id/textView3"
		android:layout_alignLeft="@+id/textView3"></TextView>
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/textView3"
		android:text="TextView" android:textSize="18sp" android:layout_below="@+id/textView1"
		android:layout_alignLeft="@+id/textView1"></TextView>
	<TextView android:layout_width="wrap_content"
		android:textSize="18sp" android:text="TextView" android:layout_height="wrap_content"
		android:id="@+id/textView1" android:layout_below="@+id/imageView1"
		android:layout_alignParentLeft="true"></TextView>
	<TextView android:layout_width="wrap_content"
		android:textSize="18sp" android:text="TextView" android:layout_height="wrap_content"
		android:id="@+id/textView4" android:layout_above="@+id/textView3"
		android:layout_alignParentRight="true" android:layout_marginRight="88dp"></TextView>
<Button android:id="@+id/walk" android:text="行走路线" style="@style/button" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_below="@+id/textView2" android:layout_alignParentLeft="true"></Button>
</RelativeLayout>
